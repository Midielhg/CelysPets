{
  "name": "celyspets-api",
  "script": "index.js",
  "cwd": "./",
  "instances": 1,
  "exec_mode": "fork",
  "watch": false,
  "max_memory_restart": "1G",
  "env": {
    "NODE_ENV": "production",
    "PORT": "3001"
  },
  "log_file": "./logs/app.log",
  "out_file": "./logs/out.log",
  "error_file": "./logs/error.log",
  "log_date_format": "YYYY-MM-DD HH:mm:ss",
  "restart_delay": 4000,
  "max_restarts": 10,
  "min_uptime": "10s"
}
